.header-wrapper {
    width: 100%;
    height: 74px;
    background: #eaebed;
    // filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff',endColorstr='#d4d7db');
    background: -webkit-gradient(linear,left top,left bottom,from(#fff),to(#d4d7db));
    background: -moz-linear-gradient(top,#fff,#d4d7db);
    box-shadow: 0 4px 4px rgba(0,0,0,.2);

    .header-containers {
        width: 1200px;
        margin: 0 auto;
    }

    .header-logo {
        float: left;
        h1 {
            height: 74px;
            line-height: 74px;
        }
    }

    .header-list {
        float: right;
        a {
            display: inline-block;
            width: 90px;
            height: 74px;
            line-height: 74px;
            color: #2b2b2b;
        }
    }
}
